<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true">

    <com.lxf.ichat.view.base.BaseCircleImageView
        android:id="@+id/item_list_fragment_mine_other_CIV_icon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:src="@mipmap/setting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:outCircleColor="#8d2ad4"
        tools:outCircleWidth="2dp" />

    <TextView
        android:id="@+id/item_list_fragment_mine_other_TV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="设置"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/item_list_fragment_mine_other_CIV_icon"
        app:layout_constraintStart_toEndOf="@+id/item_list_fragment_mine_other_CIV_icon"
        app:layout_constraintTop_toTopOf="@+id/item_list_fragment_mine_other_CIV_icon"
        app:layout_constraintVertical_bias="0.43" />

</android.support.constraint.ConstraintLayout>
